  mytalks:
  - level: 0
    questiom:
      sentence: "\u55B2!\u5F9E\u56DE\u4F86\u7D93\u71DF\u96DC\u8CA8\u5E97\u9084\u7FD2\u6163\u55CE?\u6700\u8FD1\u4F86\u9001\u8CA8\u7684\u6642\u5019\u770B\u4F60\u9ED1\u773C\u5708\u90FD\u6EFF\u91CD\u7684\u3002"
      CardOpens: 0100000002000000
    answers:
    - sentence: "\u6069\u2026\u2026\u770B\u4F86\u4F60\u904E\u5F97\u9084\u4E0D\u932F\uFF0C\u90A3\u5C31\u597D\u3002\u4E0D\u904E\u798F\u4F2F\u5C31\u4E0D\u662F\u9019\u6A23\u4E86\uFF0C\u5F9E\u4F60\u963F\u516C\u8D70\u4E86\u4EE5\u5F8C\u4ED6\u604D\u60DA\u7684\u6642\u9593\u5C31\u8B8A\u5F97\u5F88\u591A\uFF0C\u93AE\u88E1\u7684\u4EBA\u90FD\u5F88\u64D4\u5FC3\u3002"
      OpenLevel: 1
      NeedToTrigger: 01000000
    - sentence: "\u54C8\u54C8\uFF0C\u7562\u7ADF\u5F88\u591A\u6771\u897F\u90FD\u548C\u4EE5\u524D\u4E0D\u4E00\u6A23\u4E86\uFF0C\u8981\u5FD9\u7684\u4E8B\u60C5\u7576\u7136\u591A\u56C9!"
      OpenLevel: 2
      NeedToTrigger: 02000000
  - level: 1
    questiom:
      sentence: "\u524D\u9663\u5B50\u6211\u5E6B\u6211\u7238\u9001\u8CA8\u7684\u6642\u5019\u4ED6\u76F4\u63A5\u5F9E\u5C0D\u5411\u8ECA\u9053\u5207\u904E\u4F86\uFF0C\u6211\u5FEB\u5687\u6B7B\u4E86\u3002%\u4E0D\u904E\u4ED6\u5012\u662F\u9023\u770B\u90FD\u6C92\u770B\u5C31\u9A0E\u8D70\u4E86\uFF0C\u5509\u2026\u2026"
      CardOpens: 0400000005000000
    answers:
    - sentence: "\u662F\u554A\uFF0C\u7576\u6642\u6211\u958B\u8457\u8CA8\u8ECA\uFF0C\u9060\u9060\u7684\u5C31\u770B\u5230\u798F\u4F2F\u86C7\u884C\u9A0E\u904E\u4F86\uFF0C\u90A3\u6642\u5019\u5C31\u89BA\u5F97\u5F88\u6050\u6016\u4E86!%\u9084\u597D\u6211\u5F8C\u4F86\u6709\u653E\u6162\u901F\u5EA6\u3002\u798F\u4F2F\u7D93\u904E\u7684\u6642\u5019\u6211\u597D\u50CF\u9084\u807D\u5230\u4ED6\u9084\u5728\u54FC\u6B4C\u2026\u2026"
      OpenLevel: 3
      NeedToTrigger: 04000000
    - sentence: "\u54FC?\u5594\uFF0C\u5C0D\u554A\u3002\u6211\u5176\u5BE6\u660E\u5E74\u624D\u80FD\u8003\u99D5\u7167\uFF0C\u4E0D\u904E\u6C92\u95DC\u4FC2\u5566!\u6797\u53D4\u548C\u738B\u4F2F\u77E5\u9053\u4E86\u4E5F\u6C92\u6709\u958B\u6211\u55AE\uFF0C\u800C\u4E14\u6211\u5DF2\u7D93\u958B1\u5E74\u4E86\uFF0C\u6280\u8853\u4E0A\u6C92\u554F\u984C\u7684\u5566\u3002"
      OpenLevel: 3
      NeedToTrigger: 05000000
  - level: 2
    questiom:
      sentence: "\u8981\u662F\u4F60\u9019\u500B\u75B2\u618A\u7684\u6A23\u5B50\u88AB\u7389\u862D\u5B38\u770B\u5230\uFF0C\u4F60\u5927\u6982\u6703\u88AB\u5FF51\u500B\u5C0F\u6642\u5427\u3002%\u4E0D\u904E\u5979\u5FF5\u6B78\u5FF5\uFF0C\u4F60\u9694\u5929\u5927\u6982\u6703\u6536\u5230\u5979\u71C9\u7684\u4E00\u5927\u934B\u96DE\u6E6F\u3002\u54C8\u54C8\u3002"
      CardOpens: 0700000008000000
    answers:
    - sentence: "\u7389\u84EE\u5B38\u5C31\u662F\u9019\u6A23\u3002\u518D\u52A0\u4E0A\u524D\u9663\u5B50\u4F60\u963F\u516C\u624D\u8D70\uFF0C\u5979\u66F4\u5E0C\u671B\u5927\u5BB6\u90FD\u53EF\u4EE5\u5065\u5065\u5EB7\u5EB7\u7684\u3002"
      OpenLevel: 3
      NeedToTrigger: 07000000
    - sentence: "\u54C8\u54C8\u54C8\uFF0C\u6211\u5DF2\u7D93\u53EF\u4EE5\u60F3\u50CF\u5230\u7389\u84EE\u5B38\u9084\u5728\u5E97\u9580\u53E3\u5C31\u908A\u53EB\u4F60\u7684\u540D\u5B57\u3001\u908A\u7AEF\u8457\u96DE\u6E6F\u8D70\u9032\u4F86\u7684\u756B\u9762\u4E86!"
      OpenLevel: 3
      NeedToTrigger: 08000000
  - level: 3
    questiom:
      sentence: "\u6597\u516D\u7684\u5927\u5BB6\u771F\u7684\u90FD\u5F88\u597D\u3002\u53EA\u662F\u4E00\u60F3\u5230\u53BB\u53F0\u5317\u4E4B\u5F8C\u53EF\u80FD\u5C31\u611F\u53D7\u4E0D\u5230\u9019\u6A23\u7684\u4EBA\u60C5\u5473\u5C31\u89BA\u5F97\u4EE5\u9EDE\u96E3\u904E\u3002"
      CardOpens: 0a0000000b000000
    answers:
    - sentence: "\u6211\u60F3\u53BB\u958B\u4E00\u9593\u81EA\u5DF1\u7684\u670D\u98FE\u5E97\u3002\u53F0\u5317\u4EBA\u591A\u3001\u8CC7\u6E90\u4E5F\u591A\uFF0C\u6BD4\u8F03\u5BB9\u6613\u7D2F\u7A4D\u5BE6\u529B\u548C\u540D\u6C23\u3002%\u4E0D\u904E\u6211\u73FE\u5728\u4E5F\u9084\u5728\u5B58\u9322\u5566\uFF0C\u54C8\u54C8\u54C8\u3002%\u90A3\u4ECA\u5929\u5C31\u5148\u9019\u6A23\u5427\uFF0C\u5148\u53BB\u9001\u5176\u4ED6\u8CA8\u56C9?\u63B0\u63B0!"
